#driver
new  -s driver -n misc
send-keys 'cd ~' C-m
send-keys 'ls' C-m
neww -n dotfiles
send-keys 'cd ~/.dotfiles' C-m
send-keys 'ls' C-m
neww -n playground
send-keys 'j playground' C-m
send-keys 'ls' C-m
neww -n linode
send-keys 'mosh lyell'
neww -n rnpfind
send-keys 'j rnpfind webapp' C-m
send-keys 'ls' C-m
neww -n missing
send-keys 'j missing' C-m
send-keys 'ls' C-m
selectw -t 0
